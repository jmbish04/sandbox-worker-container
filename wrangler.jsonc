{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "gh-sandbox-container",
  "main": "src/index.ts",
  "compatibility_date": "2024-10-22",
  "compatibility_flags": ["nodejs_compat"],


  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true
    }
  },
  
  // Durable Objects configuration
  "durable_objects": {
    "bindings": [
      {
        "name": "SANDBOX",
        "class_name": "Sandbox"
      },
      {
        "name": "TASK_ORCHESTRATOR",
        "class_name": "TaskOrchestratorActor"
      },
      {
        "name": "AGENT_ERROR_RECREATION",
        "class_name": "ErrorRecreationAgent"
      },
      {
        "name": "AGENT_SOLUTION_VALIDATION",
        "class_name": "SolutionValidationAgent"
      },
      {
        "name": "AGENT_TESTING",
        "class_name": "TestingAgent"
      }
    ]
  },
  
  // Workflows configuration
  "workflows": [
    {
      "name": "ERROR_WORKFLOW",
      "binding": "ERROR_WORKFLOW",
      "class_name": "ErrorAnalysisWorkflow"
    },
    {
      "name": "VALIDATION_WORKFLOW",
      "binding": "VALIDATION_WORKFLOW",
      "class_name": "ValidationWorkflow"
    },
    {
      "name": "TESTING_WORKFLOW",
      "binding": "TESTING_WORKFLOW",
      "class_name": "TestingWorkflow"
    }
  ],
  
  // Queues configuration
  "queues": {
    "producers": [
      {
        "queue": "task-queue",
        "binding": "TASK_QUEUE"
      },
      {
        "queue": "gh-sandbox-sdk-results",
        "binding": "RESULT_QUEUE"
      }
    ],
    "consumers": [
      {
        "queue": "task-queue",
        "max_batch_size": 10,
        "max_batch_timeout": 30
      },
      {
        "queue": "gh-sandbox-sdk-results",
        "max_batch_size": 10,
        "max_batch_timeout": 30
      }
    ]
  },
  
  // KV Namespace binding (using the created namespace)
  "kv_namespaces": [
    {
      "binding": "KV_STATE",
      "id": "684c6f00822b404ebe20c78b2516706f"
    }
  ],
  
  // R2 Bucket binding (using existing bucket)
  "r2_buckets": [
    {
      "binding": "R2_ARTIFACTS",
      "bucket_name": "agent-artifacts"
    }
  ],
  
  // D1 Database binding (using the created database)
  "d1_databases": [
    {
      "binding": "D1_METRICS",
      "database_id": "8311509c-5b51-4024-bf51-5e1540692ea2",
      "database_name": "agent-metrics"
    }
  ],
  
  // Durable Objects migrations
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "Sandbox",
        "TaskOrchestratorActor",
        "ErrorRecreationAgent",
        "SolutionValidationAgent",
        "TestingAgent"
      ]
    }
  ],
  
  // Assets configuration
  "assets": {
    "binding": "ASSETS",
    "directory": "./public"
  },
  
  // AI binding
  "ai": {
    "binding": "AI"
  },
  
  // Container configuration
  "containers": [
    {
      "class_name": "Sandbox",
      "instance_type": "basic",  // or standard-1, standard-2, standard-3, standard-4
      "image": "./Dockerfile"
    }
  ],
  
  // Secrets Store bindings
  // IMPORTANT: Replace YOUR_STORE_ID with your actual Secrets Store ID
  // You can find it at: https://dash.cloudflare.com/?to=/:account/secrets-store
  "secrets_store_secrets": [
    {
      "binding": "GITHUB_TOKEN",
      "store_id": "8c42fa70938644e0a8a109744467375f", 
      "secret_name": "GITHUB_TOKEN"
    },
    {
      "binding": "OPENAI_API_KEY",
      "store_id": "8c42fa70938644e0a8a109744467375f",
      "secret_name": "OPENAI_API_KEY"
    },
    {
      "binding": "ANTHROPIC_API_KEY",
      "store_id": "8c42fa70938644e0a8a109744467375f",
      "secret_name": "ANTHROPIC_API_KEY"
    },
    {
      "binding": "GEMINI_API_KEY",
      "store_id": "8c42fa70938644e0a8a109744467375f",
      "secret_name": "GEMINI_API_KEY"
    }
  ]
}
